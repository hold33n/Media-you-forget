{
  "name": "react-heroku-starter",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading and easy heroku building",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --hot --history-api-fallback --open",
    "postinstall": "webpack -p --config ./webpack-prod.config.js",
    "lint": "eslint src"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "heroku"
  ],
  "author": "Alex Vakarchuk <alex@lanp.co> (http://github.com/alexeyvakarchuk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexeyvakarchuk/react-heroku-starter/issues"
  },
  "homepage": "https://github.com/alexeyvakarchuk/react-heroku-starter",
  "dependencies": {
    "axios": "^0.21.1",
    "babel-core": "^6.0.20",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.27.3",
    "email-validator": "^1.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "firebase": "^4.3.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
